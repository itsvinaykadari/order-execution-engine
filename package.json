{
  "name": "order-execution-engine",
  "version": "1.0.0",
  "description": "DEX Order Execution Engine with WebSocket status updates",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && cp -r src/database/*.sql dist/database/",
    "start": "node dist/server.js",
    "worker": "tsx src/worker/index.ts",
    "worker:dev": "tsx watch src/worker/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "tsx src/database/migrate.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": ["dex", "order-execution", "websocket", "solana"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/websocket": "^10.0.1",
    "@types/node": "^22.5.5",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.12",
    "bullmq": "^5.13.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "pg": "^8.12.0",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "typescript": "^5.6.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1"
  }
}
